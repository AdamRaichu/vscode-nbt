{
	"name": "vscode-nbt",
	"displayName": "NBT",
	"description": "NBT Editor for VSCode",
	"version": "0.0.1",
	"publisher": "Misode",
	"enableProposedApi": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/misode/vscode-nbt.git"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCustomEditor:nbtEditor.nbt"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"customEditors": [
			{
				"viewType": "nbtEditor.nbt",
				"displayName": "NBT Viewer",
				"selector": [
					{
						"filenamePattern": "*.nbt"
					}
				],
				"priority": "default"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^3.9.4"
	},
	"dependencies": {
		"nbt": "^0.8.1",
		"node-gzip": "^1.1.2"
	}
}
